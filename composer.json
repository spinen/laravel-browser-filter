{
    "name": "spinen/laravel-browser-filter",
    "description": "Filters request based on browser type.",
    "keywords": [
        "browser hawk",
        "laravel",
        "library",
        "middleware",
        "spinen"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/cache": "5.1.*|5.2.*|5.3.*",
        "illuminate/routing": "5.1.*|5.2.*|5.3.*",
        "illuminate/support": "5.1.*|5.2.*|5.3.*",
        "mobiledetect/mobiledetectlib": "~2.8",
        "ua-parser/uap-php": "~3.4.1"
    },
    "require-dev": {
        "illuminate/http": "~5.1",
        "mockery/mockery": "~0.9.1",
        "phpunit/phpunit": "~4.0",
        "psy/psysh": "^0.5.1",
        "satooshi/php-coveralls": "^0.6.1",
        "symfony/var-dumper": "~2.7|~3.0"
    },
    "autoload": {
        "files": [
            "src/Support/helper.php"
        ],
        "psr-4": {
            "Spinen\\BrowserFilter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\BrowserFilter\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}